<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.express.model.ExpressEntity" table="express" schema="" catalog="express">
        <id name="id" column="ID"/>
        <property name="isPackage" column="IsPackage"/>
        <property name="weight" column="Weight"/>
        <property name="accepter" column="Accepter"/>
        <property name="accTel" column="AccTel"/>
        <property name="getTime" column="GetTime"/>
        <property name="outTime" column="OutTime"/>
        <property name="tranFee" column="TranFee"/>
        <property name="insuFee" column="InsuFee"/>
    </class>
</hibernate-mapping>